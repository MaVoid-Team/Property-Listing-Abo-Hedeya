			"name": "Authentication",
			"item": [
				{
					"name": "Login",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"try {",
									"  if (pm.response.code === 200) {",
									"    var jsonData = pm.response.json();",
									"    if (jsonData && jsonData.success) {",
									"      // Prefer token from JSON body (custom SessionsController)",
									"      var token = jsonData.token;",
									"      if (token) {",
									"        // Store in both environment and collection variables",
									"        pm.environment.set('auth_token', token);",
									"        pm.collectionVariables.set('auth_token', token);",
									"      } else {",
									"        // Fallback: extract token from Authorization header if present",
									"        var authHeader = pm.response.headers.get('Authorization');",
									"        if (authHeader) {",
									"          var headerToken = authHeader.replace('Bearer ', '');",
									"          pm.environment.set('auth_token', headerToken);",
									"          pm.collectionVariables.set('auth_token', headerToken);",
									"        }",
									"      }",
									"    }",
									"  }",
									"} catch (e) {",
									"  console.log('Login test script error:', e.message);",
									"}"
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
