	"item": [
		{
			"name": "Authentication",
			"item": [
				{
					"name": "Login",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 200) {",
									"    var jsonData = pm.response.json();",
									"    if (jsonData.success) {",
									"        // Extract token from Authorization header if present",
									"        var authHeader = pm.response.headers.get('Authorization');",
									"        if (authHeader) {",
									"            pm.environment.set('auth_token', authHeader.replace('Bearer ', ''));",
									"        }",
									"    }",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"admin_user\": {\n    \"email\": \"admin@propertylistings.com\",\n    \"password\": \"admin123\"\n  }\n}"
						},
						"url": {
							"raw": "{{base_url}}/api/v1/login",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"v1",
								"login"
							]
						},
						"description": "Admin login endpoint. Returns JWT token in Authorization header.\n\n**Default Credentials:**\n- Email: admin@propertylistings.com\n- Password: admin123"
					},
					"response": []
				},
				{
					"name": "Logout",
					"request": {
						"method": "DELETE",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{auth_token}}",
								"type": "text"
							}
						],
						"url": {
							"raw": "{{base_url}}/api/v1/logout",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"v1",
								"logout"
							]
						},
						"description": "Logout endpoint. Revokes the JWT token."
					},
					"response": []
				}
			],
			"description": "Authentication endpoints for admin users"
